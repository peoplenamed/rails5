- if user_signed_in?
  %nav#navigation-example.navbar.navbar-expand-lg.navbar-transparent.navbar-absolute.fixed-top
    .container-fluid
      .navbar-wrapper
        %a.navbar-brand{:href => "javascript:void(0)"} Dashboard
      %button.navbar-toggler{"aria-controls" => "navigation-index", "aria-expanded" => "false", "aria-label" => "Toggle navigation", "data-target" => "#navigation-example", "data-toggle" => "collapse", :type => "button"}
        %span.sr-only Toggle navigation
        %span.navbar-toggler-icon.icon-bar
        %span.navbar-toggler-icon.icon-bar
        %span.navbar-toggler-icon.icon-bar
      .collapse.navbar-collapse.justify-content-end
        %form.navbar-form
          .input-group.no-border
            %input.form-control{:placeholder => "Search...", :type => "text", :value => ""}/
            %button.btn.btn-default.btn-round.btn-just-icon{:type => "submit"}
              %i.material-icons search
              .ripple-container
        %ul.navbar-nav
          %li.nav-item
            %a.nav-link{:href => "javascript:void(0)"}
              %i.material-icons dashboard
              %p.d-lg-none.d-md-block
                Stats
          %li.nav-item.dropdown
            %a#navbarDropdownMenuLink.nav-link{"aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown", :href => "javscript:void(0)"}
              %i.material-icons notifications
              %span.notification 5
              %p.d-lg-none.d-md-block
                Some Actions
            .dropdown-menu.dropdown-menu-right{"aria-labelledby" => "navbarDropdownMenuLink"}
              %a.dropdown-item{:href => "javascript:void(0)"} Mike John responded to your email
              %a.dropdown-item{:href => "javascript:void(0)"} You have 5 new tasks
              %a.dropdown-item{:href => "javascript:void(0)"} You're now friend with Andrew
              %a.dropdown-item{:href => "javascript:void(0)"} Another Notification
              %a.dropdown-item{:href => "javascript:void(0)"} Another One
          %li.nav-item
            %a.nav-link{:href => "javascript:void(0)"}
              %i.material-icons person
              %p.d-lg-none.d-md-block
                Account




  %a{:href => edit_user_registration_path, :style => "margin-left:20px;", :class => 'navbar-link'}
    %span{ class: "fa fa-user-astronaut"}
    = current_user.email

  %a{"data-confirm" => "Are you sure?", "data-method" => "delete", :href => destroy_user_session_path, :rel => "nofollow", :style=>"float:right; margin-right:20px;"}
    Logout
    %span{ class: "fa fa-space-shuttle"}

- else
  = link_to "Sign up", new_user_registration_path, :class => 'navbar-link'
  |
  = link_to "Login", new_user_session_path, :class => 'navbar-link'
